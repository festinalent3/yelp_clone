<p><%= @restaurant.name %></p>
<p><%= @restaurant.description %></p>
<%= image_tag @restaurant.image.url(:medium) %>

<% @restaurant.reviews.each do |review| %>
	<%= review.thoughts %>
	<%= review.rating %>
	<%= link_to 'Delete Review', "/restaurants/#{@restaurant.id}/reviews/#{review.id}", method: :delete %>
<% end %>
